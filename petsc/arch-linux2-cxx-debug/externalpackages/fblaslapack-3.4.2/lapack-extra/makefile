SOURCEF = ilaver.f
OBJSF   = $(SOURCEF:.f=.o)

SOURCEF_SPECIAL = slamch.f dlamch.f
OBJSF_SPECIAL   = $(SOURCEF_SPECIAL:.f=.o)

lib:
	$(FC) -c $(FNOOPT) $(SOURCEF_SPECIAL)
	$(FC) -c $(FOPTFLAGS) $(SOURCEF)
	$(AR) $(AR_FLAGS) ../$(LIBNAME) $(OBJSF_SPECIAL) $(OBJSF) 
	$(RM) $(OBJSF_SPECIAL) $(OBJSF)


